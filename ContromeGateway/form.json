{
    "elements": [
        {
            "type": "Label",
            "caption": "Controme Heating Control - Gateway Component\n ",
            "link": false,
            "bold": true
        },
        {
            "type": "Label",
            "caption": "Symcon module for local control and monitoring of Controme heating systems."
        },
        {
            "type": "Label",
            "caption": "Display of rooms, sensors, actual/target temperatures, humidity, and operating mode.\nSupports reading and writing via the Controme API.\nFully integrable into IPS with timer and variable profiles.\n"
        },
        {
            "type": "Label",
            "caption": "Limitations: this gateway is tested with Controme firmware branche V2.0 (stable) from 2025-02-18 16:04:25 or later\n\n",
            "italic": true
        },
        {
            "type": "Label",
            "caption": "\nConfiguration",
            "bold": true
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "ValidationTextBox",
                    "name": "User",
                    "caption": "Username (Super-Admin from your Controme App)",
                    "width": "300px"
                },
                {
                    "type": "PasswordTextBox",
                    "caption": "Password",
                    "enabled": true,
                    "name": "Password",
                    "onChange": "",
                    "validate": "",
                    "value": "",
                    "visible": true,
                    "width": "300px"
                }
            ]
        },
        {
            "type": "ValidationTextBox",
            "name": "IPAddress",
            "caption": "Device IP (local, e.g. 192.168.1.101)",
            "width": "300px"
        },
        {
            "type": "ExpansionPanel",
            "caption": "Expert Settings...",
            "expanded": false,
            "link": false,
            "visible": true,
            "width": "",
            "items": [
                {
                    "type": "Label",
                    "caption": "Normally these settings do not need to be changed.",
                    "bold": true
                },
                {
                    "type": "Label",
                    "caption": "Controme note: API returns nothing?\nIn this case, please first check whether you are using the correct house id. For various reasons, the id is sometimes '2' and not '1'.",
                    "italic": true
                },
                {
                    "type": "NumberSpinner",
                    "caption": "House ID",
                    "digits": 0,
                    "enabled": true,
                    "hex": false,
                    "maximum": null,
                    "minimum": 1,
                    "moreDigits": false,
                    "name": "HouseID",
                    "onChange": "",
                    "suffix": "",
                    "value": 1,
                    "visible": true,
                    "width": "300px"
                },
                {
                    "type": "Label",
                    "caption": "If Controme supports HTTPS in the future, the plan is to enable it here.",
                    "italic": true
                },
                {
                    "type": "Label",
                    "caption": "Controme note:\nSince there are no certificates installed on the miniserver, it is recommended to send /set/ requests only within the local network.",
                    "italic": true
                },
                {
                    "type": "CheckBox",
                    "name": "UseHTTPS",
                    "caption": "Use HTTPS (Mini server with certificates)"
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Controme Instance Creation...",
            "expanded": false,
            "link": false,
            "name": "ExpansionPanelRooms",
            "visible": true,
            "width": "",
            "items": [
                {
                    "type": "Label",
                    "caption": "Target category for new instances"
                },
                {
                    "type": "SelectCategory",
                    "name": "TargetCategory",
                    "caption": "Select target category",
                    "value": 0
                },
                {
                    "type": "Label",
                    "caption": "IPS Central Control for Controme",
                    "bold": true
                },
                {
                    "type": "Label",
                    "caption": "The Controme Central Control instance is a global instance for controlling and monitoring the Controme system (however limited to the options, that Controme allows to be configured from outside of their system...). It does not belong to a specific room.",
                    "bold": false
                },
                {
                    "type": "Button",
                    "name": "ButtonCreateCentralInstance",
                    "caption": "Create Instance for central control",
                    "onClick": "CONGW_CreateCentralControlInstance($id);"
                },
                {
                    "type": "Label",
                    "caption": "IPS Room Thermostat for Controme",
                    "bold": true
                },
                {
                    "type": "Label",
                    "caption": "Note: if not done alread, i.e. in case there are no rooms in the room list: please import the configuration of the Controme Mini-Server first (use the button within \"Execute action...\" section below).",
                    "italic": true
                },
                {
                    "type": "List",
                    "name": "Rooms",
                    "caption": "Configured rooms within Controme Mini-Server",
                    "rowCount": 5,
                    "loadValuesFromConfiguration": true,
                    "add": false,
                    "delete": false,
                    "sort": {
                        "column": "Floor",
                        "direction": "ascending"
                    },
                    "columns": [
                        {
                            "caption": "Floor ID",
                            "name": "FloorID",
                            "width": "80px",
                            "save": true,
                            "visible": true,
                            "onClick": "CONGW_EnableDisableFormButtons($id);"
                        },
                        {
                            "caption": "Floor name",
                            "name": "Floor",
                            "width": "275px",
                            "save": true,
                            "visible": true,
                            "edit": {
                                "type": "ValidationTextBox",
                                "caption": "Floor"
                            },
                            "onClick": "CONGW_EnableDisableFormButtons($id);"
                        },
                        {
                            "caption": "Room ID",
                            "name": "RoomID",
                            "width": "50px",
                            "save": true,
                            "visible": true,
                            "onClick": "CONGW_EnableDisableFormButtons($id);"
                        },
                        {
                            "caption": "Room name",
                            "name": "Room",
                            "width": "275px",
                            "save": true,
                            "visible": true,
                            "edit": {
                                "type": "ValidationTextBox",
                                "caption": "Room"
                            },
                            "onClick": "CONGW_EnableDisableFormButtons($id);"
                        },
                        {
                            "name": "InstanceExists",
                            "caption": "Instance exists",
                            "width": "120px",
                            "save": false,
                            "visible": true,
                            "edit": {
                                "type": "CheckBox",
                                "enabled": false
                            }
                        }
                    ],
                    "values": []
                },
                {
                    "type": "Label",
                    "caption": "Note: You can rename floors and rooms here for better clarity in the IP-Symcon-Admin area. However, if you use the 'Read configuration from Controme Mini-Server' button, your changes will be overwritten with the names given by the Controme Mini-Server. The Controme Room Thermostat and Central Control only use the >>room IDs<< as unique identifiers, so changing names in the Room Thermostat does not affect the connection, as long as the IDs remain unchanged. The names used for Symcon visualizations will be taken from the \"Controme Central Control\" respectivly \"Controme Room Thermostat\" instances - you can change them there and they will not be overwritten."
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {

                            "type": "Button",
                            "name": "ButtonCreateRoomInstance",
                            "caption": "Create instance for selected room",
                            "onClick": "selected = GetSelectedRows($id, 'Rooms'); if(selected.length == 1) { CONGW_CreateRoomThermostatInstance($id, selected[0]['RoomID']); } else { alert('Please select exactly one room!'); }"                            "enabled": false
                        },
                        {
                            "type": "Label",
                            "name": "InstanceCreationResult",
                            "caption": ""
                        }
                    ]
                }
            ]
        }
    ],
    "actions": [
        {
            "type": "ExpansionPanel",
            "caption": "Execute action ...",
            "expanded": true,
            "items": [
                {
                    "type": "Label",
                    "caption": "\nCheck connection to Controme MiniServer."
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Button",
                            "caption": "Check connection",
                            "onClick": "IPS_RequestAction($id,'CheckConnection',true);",
                            "enabled": true
                        },
                        {
                            "type": "Label",
                            "name": "Result",
                            "caption": " "
                        }
                    ]
                },
                {
                    "type": "Label",
                    "caption": "\nTo create IPS Controme Central Control and Room Thermostat instances, you must first read the configuration from the Controme MiniServer."
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Button",
                            "caption": "Read configuration from Controme Mini-Server",
                            "onClick": "IPS_RequestAction($id,'FetchRoomList',true);",
                            "enabled": true
                        },
                        {
                            "type": "Label",
                            "name": "StatusInstances",
                            "caption": " "
                        }
                    ]
                },
                {
                    "type": "Label",
                    "caption": "Note:\n1) If floor or room IDs in the Controme system change after you have created instances, those instances may not receive data or may receive incorrect data.\n2) Floor and room names can be changed within the module, but IDs must remain consistent for correct operation."
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Source code, donation and licence ...",
            "expanded": true,
            "items": [
                {
                    "type": "Label",
                    "caption": "The software is free of charge for non-commercial use, I would appreciate a donation if you like the module.",
                    "bold": true
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "RowLayout",
                            "items": [
                                {
                                    "type": "Image",
                                    "image": "data:image/svg+xml;base64,",
                                    "onClick": "echo 'https://github.com/AllardLiao/';"
                                }
                            ]
                        },
                        {
                            "type": "RowLayout",
                            "items": [
                                {
                                    "type": "Image",
                                    "_image_de": "data:image/svg+xml;base64,",
                                    "image": "data:image/svg+xml;base64,",
                                    "onClick": "echo 'https://www.paypal.com/paypalme/KaiOey';"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "Label",
                    "caption": "\nLicense ",
                    "link": false,
                    "bold": true
                },
                {
                    "type": "Label",
                    "caption": "Copyright (c) 2025 Kai J. Oey aka AllardLiao"
                },
                {
                    "type": "Label",
                    "caption": "This work is licensed under a Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License."
                },
                {
                    "type": "Label",
                    "caption": "See https://creativecommons.org/licenses/by-nc-sa/4.0/ for details."
                }
            ]
        },
        {
            "name": "EchoPopup",
            "type": "PopupAlert",
            "visible": false,
            "popup": {
                "items": [
                    {
                        "type": "Label",
                        "name": "EchoMessage",
                        "caption": ""
                    }
                ]
            }
        },
        {
            "name": "EchoText",
            "type": "PopupAlert",
            "visible": false,
            "popup": {
                "items": [
                    {
                        "type": "ValidationTextBox",
                        "name": "JSONL",
                        "caption": "Definition:",
                        "multiline": true,
                        "width": "900px"
                    }
                ]
            }
        }
    ],
    "status": [
        {
            "code": 104,
            "icon": "inactive",
            "caption": "Gateway created - interface inactive"
        }
    ]
}
