{
    "elements": [
        {
            "type": "Label",
            "caption": "\nControme Central Heating Control\n",
            "link": false,
            "bold": true
        },
        {
            "type": "Label",
            "caption": "Symcon module for central heating system management.\n\nThis module displays the current system status, temperature, humidity, and operating mode for every room. You can also adjust global setpoints and monitor the overall system from this interface."
        },
        {
            "type": "Label",
            "caption": "Controme only allows setting temperatures and modes from external applications. In case you want to change other configurations please use the Controme interface."
        },
        {
            "type": "Label",
            "name": "ContromeIP",
            "link": true,
            "visible": false,
            "caption": "ip-address-of-your-controme-gateway/raumregelung-pro/"
        },
        {
            "type": "ExpansionPanel",
            "caption": "Visualisation ...",
            "items": [
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowMainElements",
                            "caption": "Show main elements (always on)",
                            "value": true,
                            "enabled": false,
                            "width": "800px"
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorMainTiles",
                            "caption": "Dashboard background color for main tiles (mode, set point and temp set point)",
                            "allowTransparent": true
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowSystemInfo",
                            "caption": "Show system information (Controme hardware & software)",
                            "value": true,
                            "width": "800px"
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorSystemInfoTile",
                            "caption": "Dashboard background color for main tiles (mode, set point and temp set point)",
                            "allowTransparent": true
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowRooms",
                            "caption": "Show room information (name, floor, termperature & setpoint)",
                            "value": true,
                            "width": "800px",
                            "onChange": "IPS_RequestAction($id, 'DisableFormField', json_encode(['FormField' => 'ShowRoomData', 'onOrOff' => $ShowRooms]));
                                         IPS_RequestAction($id, 'DisableFormField', json_encode(['FormField' => 'ShowRoomOffsets', 'onOrOff' => $ShowRooms]));
                                         IPS_RequestAction($id, 'DisableFormField', json_encode(['FormField' => 'ShowRoomOffsetsOnlyActive', 'onOrOff' => $ShowRooms]));
                                         IPS_RequestAction($id, 'DisableFormField', json_encode(['FormField' => 'ShowRoomSensors', 'onOrOff' => $ShowRooms]));
                                         "
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorRoomTiles",
                            "caption": "Dashboard background color for rooms",
                            "allowTransparent": true
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorFloorTiles",
                            "caption": "Dashboard background color for floors",
                            "allowTransparent": true
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowRoomData",
                            "caption": "Show more room data (humidity & mode)",
                            "value": true,
                            "width": "800px",
                            "enabled": "{ShowRooms}"
                        },
                        {
                            "type": "Label",
                            "caption": "Same tile as 'show room info'"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowRoomOffsets",
                            "caption": "Show room offset information (weather, timer, etc.)",
                            "value": false,
                            "width": "800px",
                            "enabled": "{ShowRooms}"
                        },
                        {
                            "type": "Label",
                            "caption": "Same tile as 'show room info'"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowRoomOffsetsOnlyActive",
                            "caption": "Show only active room offset information",
                            "value": false,
                            "width": "800px",
                            "enabled": "{ShowRooms}"
                        },
                        {
                            "type": "Label",
                            "caption": "Same tile as 'show room info'"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowRoomSensors",
                            "caption": "Show room sensor data (room thermostat, return flow sensors)",
                            "value": true,
                            "width": "800px",
                            "enabled": "{ShowRooms}"
                        },
                        {
                            "type": "Label",
                            "caption": "Same tile as 'show room info'"
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowVTR",
                            "caption": "Show flow temperature (actual, setpoint, termperature details) - not jet implemented :-(",
                            "value": false,
                            "width": "800px",
                            "enabled": false
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowTimer",
                            "caption": "Show Controme timer (planned heating periods) - not jet implemented :-(",
                            "value": false,
                            "width": "800px",
                            "enabled": false
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "CheckBox",
                            "name": "ShowCalendar",
                            "caption": "Show Controme calendar (entries from installed/linked calendars - not jet implemented :-(",
                            "value": false,
                            "width": "800px",
                            "enabled": false
                        }
                    ]
                },
                {
                    "type": "NumberSpinner",
                    "caption": "Duration of popup message (setting was successfull ... or not)",
                    "digits": 0,
                    "enabled": true,
                    "hex": false,
                    "maximum": 120,
                    "minimum": 3,
                    "moreDigits": false,
                    "name": "DurationOfMessagePopup",
                    "onChange": "",
                    "suffix": "Sek.",
                    "value": 8,
                    "visible": true,
                    "width": "500px"
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Label",
                            "caption": "Color of the forground (text) in the dashboard",
                            "width": "800px"
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorText",
                            "caption": "Text color",
                            "allowTransparent": false
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Label",
                            "caption": "Color of the 'mode' button in the dashboard",
                            "width": "800px"
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorModeButton",
                            "caption": "Mode-button color",
                            "allowTransparent": false
                        }
                    ]
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Label",
                            "caption": "Color of the 'temperature' buttons in the dashboard",
                            "width": "800px"
                        },
                        {
                            "type": "SelectColor",
                            "name": "VisuColorTempButtons",
                            "caption": "Text color",
                            "allowTransparent": false
                        }
                    ]
                },
                {
                    "type": "Button",
                    "caption": "Reset to default values",
                    "onClick": "IPS_RequestAction($id,'ResetToDefault',true);",
                    "enabled": true
                }
            ]
        },
        {
            "type": "ExpansionPanel",
            "caption": "Advanced settings ...",
            "items": [
                {
                    "type": "CheckBox",
                    "name": "AutoUpdate",
                    "caption": "Automatically update system values"
                },
                {
                    "type": "NumberSpinner",
                    "name": "UpdateInterval",
                    "caption": "Update interval",
                    "suffix": "minutes"
                }
            ]
        }
    ],
    "actions": [
        {
            "type": "Label",
            "caption": "\nCheck connection to Controme Gateway and Controme Mini-Server.\nNote: this checks reading and writing. However, reading works also without any Controme license, writing requires a valid license! In case you only want to read, that might work - check it using the \"Test read room data\"-button!\n"
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Button",
                    "caption": "Check connection",
                    "onClick": "IPS_RequestAction($id,'CheckConnection',true);",
                    "enabled": true
                },
                {
                    "type": "Label",
                    "name": "Result",
                    "caption": " "
                }
            ]
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "Button",
                    "caption": "Read Data Now",
                    "onClick": "IPS_RequestAction($id,'UpdateData',true);",
                    "enabled": true
                },
                {
                    "type": "Label",
                    "name": "ResultUpdate",
                    "caption": " "
                }
            ]
        },

        {
            "type": "ExpansionPanel",
            "caption": "Test Controme data delivery...",
            "expanded": true,
            "items": [
                {
                    "type": "Label",
                    "caption": "You can test data connection to Controme by setting a room id and try to fetch the data for that room from Controme Mini-Server."
                },
                {
                    "type": "NumberSpinner",
                    "caption": "Floor-ID",
                    "enabled": true,
                    "name": "FloorID",
                    "onChange": "",
                    "validate": "",
                    "value": 1,
                    "minimum": 1,
                    "visible": false,
                    "width": "100px"
                },
                {
                    "type": "NumberSpinner",
                    "caption": "Room-ID",
                    "enabled": true,
                    "name": "RoomID",
                    "onChange": "",
                    "validate": "",
                    "value": 1,
                    "minimum": 1,
                    "visible": true,
                    "width": "100px"
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "Button",
                            "caption": "Test read room data",
                            "onClick": "IPS_RequestAction($id,'TestReadRoomData',true);",
                            "enabled": true
                        },
                        {
                            "type": "Label",
                            "name": "ResultTestRead",
                            "caption": " "
                        }
                    ]
                }
            ]
        },

        {
            "type": "ExpansionPanel",
            "caption": "Source code, donation and licence ...",
            "expanded": true,
            "items": [
                {
                    "type": "Label",
                    "caption": "The software is free of charge for non-commercial use, I would appreciate a donation if you like the module.",
                    "bold": true
                },
                {
                    "type": "RowLayout",
                    "items": [
                        {
                            "type": "RowLayout",
                            "items": [
                                {
                                    "type": "Image",
                                    "image": "data:image/svg+xml;base64,",
                                    "onClick": "echo 'https://github.com/AllardLiao/';"
                                }
                            ]
                        },
                        {
                            "type": "RowLayout",
                            "items": [
                                {
                                    "type": "Image",
                                    "_image_de": "data:image/svg+xml;base64,",
                                    "image": "data:image/svg+xml;base64,",
                                    "onClick": "echo 'https://www.paypal.com/paypalme/KaiOey';"
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "Label",
                    "caption": "\nLicense ",
                    "link": false,
                    "bold": true
                },
                {
                    "type": "Label",
                    "caption": "Copyright (c) 2025 Kai J. Oey aka AllardLiao"
                },
                {
                    "type": "Label",
                    "caption": "This work is licensed under a Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License."
                },
                {
                    "type": "Label",
                    "caption": "See https://creativecommons.org/licenses/by-nc-sa/4.0/ for details."
                }
            ]
        },
        {
            "name": "EchoPopup",
            "type": "PopupAlert",
            "visible": false,
            "popup": {
                "items": [
                    {
                        "type": "Label",
                        "name": "EchoMessage",
                        "caption": ""
                    }
                ]
            }
        },
        {
            "name": "EchoText",
            "type": "PopupAlert",
            "visible": false,
            "popup": {
                "items": [
                    {
                        "type": "ValidationTextBox",
                        "name": "JSONL",
                        "caption": "Definition:",
                        "multiline": true,
                        "width": "900px"
                    }
                ]
            }
        }
    ],
    "status": [
        { "code": 201, "icon": "error",     "caption": "No connection to Controme Mini-Server" },
        { "code": 202, "icon": "error",     "caption": "Authentication failed" },
        { "code": 203, "icon": "error",     "caption": "Communication not normal" }
    ]
}
